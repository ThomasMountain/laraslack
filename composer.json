{
    "name": "mountain/laraslack",
    "homepage": "https://github.com/ThomasMountain/laraslack",
    "description": "A simple package to automatically send alerts to a slack channel",
    "keywords": [
        "laravel",
        "slack"
    ],
    "authors": [
        {
            "name": "ThomasMountain",
            "homepage": "https://github.com/ThomasMountain"
        }
    ],
    "support": {
        "issues": "https://github.com/ThomasMountain/laraslack/issues",
        "source": "https://github.com/ThomasMountain/laraslack"
    },
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "ThomasMountain\\LaraSlack\\": "src/"
        }
    },
    "extra": {
